{
  "name": "yaniv-counter",
  "version": "1.1.2",
  "main": "node_modules/expo/AppEntry.js",
  "engines": {
    "npm": "⚠️npmではなくyarnを使ってください⚠️"
  },
  "scripts": {
    "start": "expo start",
    "ios": "expo start --ios",
